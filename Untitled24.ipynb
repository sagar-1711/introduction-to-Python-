    "def sumArray(a,n):\n",
    "    s=a[n]\n",
    "    \n",
    "    smallarray=a[1:]\n",
    "    sum=s+sumArray(smallarray)\n",
    "    return sum\n",
    "    \n",
    "\n",
    "\n",
    "from sys import setrecursionlimit\n",
    "setrecursionlimit(11000)\n",
    "n=int(input())\n",
    "arr=[int(i) for i in input().split()]\n",
    "print(sumArray(arr))\n"
   ]
